using System;

class Program
{
    static void Main()
    {
       Console.WriteLine(" Enter a string");
        string text = Console.ReadLine();

        int wordscount = text.Split(new char[] { ' ','\t','\n' },StringSplitOptions.RemoveEmptyEntries).Length;
        Console.WriteLine("no of string : " + wordscount);
       
    }
}
